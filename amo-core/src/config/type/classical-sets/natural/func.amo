fix func diff_up (n_1 : Nat) -> (n_2 : Nat) -> Nat {
  return (
    if (n_1 <= n_2) zero (__diff_up_no_check__ n_1 n_2)
  )
}

// Unchecked precondition: n_1 > n_2
fix func __diff_up_no_check__ (n_1 : Nat) -> (n_2 : Nat) -> Nat {
  return (
    match n_1 , n_2 with
      | _        , zero     -> n_1
      | succ p_1 , succ p_2 -> diff_up_no_check p_1 , p_2
  )
}
