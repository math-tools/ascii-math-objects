/***
???

id < x et id < y  donne :

op id y < op x y
==>
y < op x y
==>
id < op x y
***/


for_toks_explicit_list kind [: | Compo | Abelian | Add | Prod :]
{
  type_ext_from "OrderedMonoid{kind}"
                ("Monoid{kind}" + "OrderedMagma")

  type_ext_from "TotalOrderedMonoid{kind}"
                ("Monoid{kind}" + "TotalOrderedMagma")
}
